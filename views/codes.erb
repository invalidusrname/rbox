<div id="codes">
  <div class="toolbar">
    <a class="back">Back</a>
    <h1>Codes</h1>
  </div>
  <ul class="plastic">
    <% for info in @codes %>
      <li class="<%= info[:status] %>">
        <%= info[:code] %>
        <small><%= [info[:date] %></small>
      </li>
    <% end %>
  </ul>
  <div class="info">
    Served at <%= Time.now %>
  </div>
</div>